<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color
        x:Key="DarkGrayColor">#424242</Color>
    <SolidColorBrush
        x:Key="DarkGrayBrush"
        Color="{StaticResource DarkGrayColor}" />

    <Color
        x:Key="WhiteColor">#FFFFFF</Color>
    <SolidColorBrush
        x:Key="WhiteBrush"
        Color="{StaticResource WhiteColor}" />

    <Color
        x:Key="BlackColor">#000000</Color>
    <SolidColorBrush
        x:Key="BlackBrush"
        Color="{StaticResource BlackColor}" />

    <Color
        x:Key="LightBlueColor">#0090C1</Color>
    <SolidColorBrush
        x:Key="LightBlueBrush"
        Color="{StaticResource LightBlueColor}" />

    <SolidColorBrush
        x:Key="BlueVioletBrush"
        Color="BlueViolet" />

    <Color
        x:Key="HoverDarkGrayColor">#626262</Color>
    <SolidColorBrush
        x:Key="HoverDarkGrayBrush"
        Color="{StaticResource HoverDarkGrayColor}" />

    <Color
        x:Key="PressedDarkGrayColor">#727272</Color>
    <SolidColorBrush
        x:Key="PressedDarkGrayBrush"
        Color="{StaticResource PressedDarkGrayColor}" />

    <Color
        x:Key="GrayColor">#999</Color>
    <SolidColorBrush
        x:Key="GrayBrush"
        Color="{StaticResource GrayColor}" />

</ResourceDictionary>